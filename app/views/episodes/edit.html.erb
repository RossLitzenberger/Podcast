<div id="podcast_show">
  <div id="show_banner">
    <div class="wrapper_skinny">
      <h1>The Rocketship Podcast Dash</h1>
      <a class="subscribe_link" href="#">iTunes</a>
      <a class="subscribe_link" href="#">Stitcher</a>
      <a class="subscribe_link" href="#">PodBay</a>
    </div>
  </div>
  <div id="links">
    <div class="wrapper_skinny">
      <ul>
        <li><%= link_to "Episodes", authenticated_root_path%></li>
        <li><%= link_to "New Episodes", new_podcast_episode_path(@podcast) %></li>
        <li><%= link_to "Account Settings", edit_podcast_registration_path%></li>
        <li><%= link_to "View Podcast", podcast_path(current_podcast)%></li>
      </ul>
    </div>
  </div>
  <div id="form">
    <div class="wrapper_skinny">
      <%= render 'form' %>
    </div>
  </div>
</div>
